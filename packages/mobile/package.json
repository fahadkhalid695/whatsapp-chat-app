{
  "name": "mobile",
  "version": "1.0.0",
  "description": "React Native mobile application for WhatsApp chat app",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "dev": "react-native start",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace mobile.xcworkspace -scheme mobile -configuration Release",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-native": "^0.72.3",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "react-native-screens": "^3.22.1",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-gesture-handler": "^2.12.1",
    "react-native-vector-icons": "^10.0.0",
    "react-native-elements": "^3.4.3",
    "socket.io-client": "^4.7.2",
    "axios": "^1.4.0",
    "zustand": "^4.4.1",
    "react-hook-form": "^7.45.2",
    "@react-native-async-storage/async-storage": "^1.19.1",
    "react-native-image-picker": "^5.6.0",
    "react-native-permissions": "^3.8.4",
    "react-native-contacts": "^7.0.8",
    "react-native-document-picker": "^9.1.1",
    "react-native-video": "^5.2.1",
    "@react-native-netinfo/netinfo": "^9.4.1",
    "react-native-fs": "^2.20.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-native": "^0.72.2",
    "@react-native-community/eslint-config": "^3.2.0",
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/runtime": "^7.22.6",
    "babel-jest": "^29.6.1",
    "jest": "^29.6.1",
    "metro-react-native-babel-preset": "^0.76.7",
    "@testing-library/react-native": "^12.1.3",
    "react-test-renderer": "^18.2.0"
  }
}